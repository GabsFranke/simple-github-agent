# Google AI API Key (get from https://aistudio.google.com/apikey)
GOOGLE_API_KEY=your_google_ai_api_key_here

# Message Queue Configuration
# For self-hosted/development: use "redis"
# For cloud production: use "pubsub"
QUEUE_TYPE=redis
REDIS_URL=redis://localhost:6379

# GitHub App Configuration
GITHUB_APP_ID=your_app_id_here
GITHUB_CLIENT_ID=your_client_id_here
GITHUB_INSTALLATION_ID=your_installation_id_here
GITHUB_WEBHOOK_SECRET=your_webhook_secret_here
GITHUB_PRIVATE_KEY="-----BEGIN RSA PRIVATE KEY-----
Your private key here
-----END RSA PRIVATE KEY-----"

# For local testing (alternative to App auth)
GITHUB_TOKEN=your_personal_access_token_here

# GCP Configuration (for production)
GCP_PROJECT_ID=your-gcp-project
PUBSUB_TOPIC=agent-requests
PUBSUB_SUBSCRIPTION=agent-requests-sub
VERTEX_AI_LOCATION=us-central1

# Service Configuration
PORT=8080
LOG_LEVEL=INFO

# Local Testing
TEST_REPO=owner/repo
TEST_ISSUE=1
TEST_COMMAND=/agent create a PR for this
